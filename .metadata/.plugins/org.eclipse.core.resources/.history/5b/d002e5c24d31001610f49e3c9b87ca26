package info.test;

import static org.junit.Assert.*;

import org.junit.Test;

import info.dao.ICartProductDao;
import info.model.Category;
import info.model.ShopDi;

public class TestCategoryDao extends BaseTest {
	private ICartProductDao categoryDao;
	
	@ShopDi
	public void setCategoryDao(ICartProductDao categoryDao) {
		this.categoryDao = categoryDao;
	}

	@Test
	public void test() {
		Category category = new Category();
		category.setName("服装");
		categoryDao.add(category);
		Category category = new Category();
		category.setName("夏季服装");
		categoryDao.add(category);
		category = new Category();
		category.setName("冬季服装");
		categoryDao.add(category);
	}

}
